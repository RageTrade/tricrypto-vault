    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.0",
    "ethereum-waffle": "^3.3.0",
    "ethers": "5.7.2",
    "fs-extra": "^10.1.0",
    "hardhat": "^2.12.2",
    "hardhat-contract-sizer": "^2.1.1",
    "hardhat-deploy": "^0.10.5",
    "hardhat-gas-reporter": "^1.0.4",
